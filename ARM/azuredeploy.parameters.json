{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vnetExistingName": {
            "value": "myvnet" 
        },
        "vnetExistingSubnet": {
            "value": "myvnet-subnet" 
        },
        "aciStorageAccountName": {
            "value": "mystorageaccount" 
        },
        "aciContainerGroupName": {
            "value": "mycontainergroup" 
        },
        "tailscaleHostname": {
            "value": "tailscale"
        },
        "tailscaleAdvertiseRoutes": {
            "value": "10.0.0.0/24,10.10.0.10.0/24" 
        },
        "tailscaleAuthKey": {
            "value": "" 
        },
        "containerSize": {
            "value": "Small" 
        },
        "containerRegistry": {
            "value": "DockerHub"
        },
        "tailscaleImageRepository": {
            "value": "myacr.azurecr.io/tailscale"
        },
        "tailscaleImageTag": {
            "value": "latest"
        },
        "tailscaleRegistryUsername": {
            "value": "" 
        },
        "tailscaleRegistryPassword": {
            "value": "" 
        }
    }
}